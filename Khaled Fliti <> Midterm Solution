import urllib.request
import validators
def openTab(lst):
  dict = {"Title": "", "url": "", "parent_tab": 0, "child_tab": []}
  dict["Title"] = input("Enter title: ")
  dict["url"] = input("Enter URL start with 'http://': ")
  url = dict["url"]
  valid = validators.url(url)
  while (not valid):
    print("Invalid URL , Please try again ")
    dict["url"] = input("Enter URL start with 'http://: ")
    valid = validators.url(url)
    url = dict["url"]
  lst.append(dict)
  return lst
